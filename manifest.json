{
	"name": "redirectionBlocker",
	"version": "0.1",
	"manifest_version": 2,
	"description": "alert the user whenever the redriction happens",
	"icons":{

	}
	"browser_action"{
		"default_title": "redirectionBlocker",
		"default_icon": "",
		"default_title": "Click here!"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": [
		"background",
		"tabs",
		"http://*/*",
		"https://*/*"
	]
	
}